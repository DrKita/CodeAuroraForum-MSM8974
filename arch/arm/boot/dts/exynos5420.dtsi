		compatible = "samsung,exynos5-fimd";
		interrupt-parent = <&combiner>;
		reg = <0x14400000 0x40000>;
		interrupts = <18 5>, <18 4>, <18 6>;
		clocks = <&clock 147>, <&clock 421>, <&clock 1024>,
