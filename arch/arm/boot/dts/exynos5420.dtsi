		compatible = "samsung,exynos5420-usb2phy";
		reg = <0x12130000 0x100>;
		clocks = <&clock 1>, <&clock 365>;
		clock-names = "ext_xtal", "usbhost";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		usbphy-sys {
			reg = <0x10040704 0xc>,
			      <0x10050230 0x4>;
		};
	};

	usb@12400000 {
		compatible = "samsung,exynos5250-dwusb3";
		clocks = <&clock 367>;
