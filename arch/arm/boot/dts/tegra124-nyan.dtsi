	ahub {
		i2s@70301100 {
			status = "okay";
		};
	};

	sdhci@700b0000 {
		power-gpios = <&gpio TEGRA_GPIO(X, 7) GPIO_ACTIVE_HIGH>;
		bus-width = <4>;
		no-1-8-v;
		non-removable;
		status = "okay";
		nvidia,no-runtime-suspend;
		nvidia,host-off-card-on;
		keep-power-in-suspend;
	};

	sdhci@700b0400 {
		power-gpios = <&gpio TEGRA_GPIO(R, 0) GPIO_ACTIVE_HIGH>;
		cd-gpios = <&gpio TEGRA_GPIO(V, 2) GPIO_ACTIVE_HIGH>;
		no-1-8-v;
		bus-width = <4>;
		status = "okay";
		vmmc-supply = <&vddio_sdmmc3>;
	};

	sdhci@700b0600 {
		bus-width = <8>;
		no-1-8-v;
		non-removable;
		status = "okay";
	};

	dfll@70110000 {
		vdd-supply = <&vdd_cpu>;
		board-params = <&{/cpu_dfll_board_params}>;
		pmic-integration = <&{/cpu_dfll_pmic_integration}>;
		status = "okay";
	};

