	pmc {
		nvidia,invert-interrupt;
	};

	sdhci@700b0000 {
		power-gpios = <&gpio 191 0>;
		reset-gpios = <&gpio 229 0>;
		bus-width = <4>;
		no-1-8-v;
		non-removable;
		status = "okay";
	};

	sdhci@700b0400 {
		cd-gpios = <&gpio 170 0>; /* gpio PV2 */
		no-1-8-v;
		bus-width = <4>;
		status = "okay";
		vmmc-supply = <&vddio_sdmmc3>;
